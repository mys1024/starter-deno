{
  "name": "@mys/starter-deno",
  "version": "0.1.3",
  "tasks": {
    "run": "deno run src/main.ts",
    "run:watch": "deno run --watch src/main.ts",
    "cache": "deno cache --lock-write src/main.ts test/**/*.test.ts",
    "cache:reload": "deno cache --reload src/main.ts test/**/*.test.ts",
    "test": "deno test",
    "test:watch": "deno test --watch",
    "test:cov": "deno test --coverage && deno coverage && deno coverage --html",
    "bump": "deno run -A jsr:@mys/bump@1",
    "publish:dry": "deno publish --dry-run"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@0.220"
  },
  "exports": {
    ".": "./src/main.ts"
  },
  "publish": {
    "include": [
      "src",
      "README*.md",
      "LICENSE",
      "deno.json"
    ]
  }
}
