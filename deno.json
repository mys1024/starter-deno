{
  "name": "",
  "version": "0.1.0",
  "tasks": {
    "run": "deno run src/main.ts",
    "run:watch": "deno run --watch src/main.ts",
    "cache": "deno cache --lock-write src/main.ts src/**/*.test.ts",
    "cache:reload": "deno cache --reload src/main.ts src/**/*.test.ts",
    "test": "deno test",
    "test:watch": "deno test --watch",
    "test:cov": "deno test --coverage && deno coverage && deno coverage --html",
    "publish": "deno publish"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@0.216"
  },
  "exports": {
    ".": "./src/main.ts"
  }
}
