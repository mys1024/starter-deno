{
  "name": "starter-deno",
  "version": "0.0.0",
  "description": "A Deno stater template ðŸ¦•",
  "type": "module",
  "author": "mys1024",
  "packageManager": "pnpm@10.4.0",
  "license": "MIT",
  "homepage": "https://github.com/mys1024/starter-deno#readme",
  "repository": "https://github.com/mys1024/starter-deno.git",
  "keywords": [
    "deno",
    "starter",
    "template"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "node scripts/prepare.js",
    "clean": "node scripts/clean.js",
    "build": "pnpm run prepare && tsup src/main.ts --format cjs,esm --dts",
    "build:clean": "pnpm run build && pnpm run clean"
  },
  "types": "./dist/main.d.ts",
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    }
  },
  "devDependencies": {
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  }
}
